	month = dec,
	year = {2020},
	keywords = {Adaptive neural networks, Concept drift, Dynamic and non-stationary environments, Electrical grid flexibility, Renewable energy resources},
	pages = {6111--6154},
	file = {Full Text PDF:C\:\\Users\\Jacob\\Zotero\\storage\\4848WWKS\\Hammami et al. - 2020 - Neural networks for online learning of non-station.pdf:application/pdf},
}

