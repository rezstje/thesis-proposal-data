	author = {Tazi, Khadija and Abbou, Fouad Mohamed and Abdi, Farid},
	month = feb,
	year = {2020},
	keywords = {Renewable energy sources, Consensus, Microgrid, Multi-agent systems, Optimization and learning algorithms, Performance indicators},
	pages = {1233--1292},
	file = {Full Text PDF:C\:\\Users\\Jacob\\Zotero\\storage\\2JGIPFMZ\\Tazi et al. - 2020 - Multi-agent system for microgrids design, optimiz.pdf:application/pdf},
}

