	author = {Himeur, Yassine and Elnour, Mariam and Fadli, Fodil and Meskin, Nader and Petri, Ioan and Rezgui, Yacine and Bensaali, Faycal and Amira, Abbes},
	month = jun,
	year = {2023},
	keywords = {Artificial intelligence, Deep learning, Big data analytics, Building automation and management system, Computing platforms, Evaluation metrics},
	pages = {4929--5021},
	file = {Full Text PDF:C\:\\Users\\Jacob\\Zotero\\storage\\FC5PCZWP\\Himeur et al. - 2023 - AI-big data analytics for building automation and .pdf:application/pdf},
